    SQLITE3_CFLAGS=`$PKG_CONFIG --cflags sqlite3`

    LIBS="$SQLITE3_LIBS $LIBS"
    CPPFLAGS="$SQLITE3_CFLAGS $CPPFLAGS"
    have_sqlite3=yes
    AC_MSG_RESULT([yes])
  else
