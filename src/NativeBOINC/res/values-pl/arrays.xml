<resources>

	<!-- Screen rotation mode -->
	<string-array name="prefScreenOrientationDisplay">
		<item>Domyślny systemowy</item>
		<item>Wymuś portrait</item>
		<item>Wymuś landscape</item>
		<item>Automatyczne obracanie</item>
	</string-array>
	<string-array name="prefScreenOrientationValues"> 
		                 <!-- values from android.content.pm.ActivityInfo: -->
		<item>-1</item>  <!-- SCREEN_ORIENTATION_UNSPECIFIED -->
		<item>1</item>   <!-- SCREEN_ORIENTATION_PORTRAIT    -->
		<item>0</item>   <!-- SCREEN_ORIENTATION_LANDSCAPE   -->
		<item>4</item>   <!-- SCREEN_ORIENTATION_SENSOR      -->
	</string-array>

	<!-- Automatic updates of display -->
	<string-array name="prefAutoUpdatesDisplay">
