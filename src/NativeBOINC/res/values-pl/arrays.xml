	<!-- Run modes for client (display) -->
	<string-array name="clientModeRun">
		<item>Działaj zawsze</item>
		<item>Działaj na podstawie preferencji</item>
		<item>Wstrzymaj</item>
	</string-array>
	<!-- Network modes for client (display) -->
	<string-array name="clientModeNetwork">
		<item>Aktywność sieciowa zawsze</item>
		<item>Aktywność sieciowa na podstawie preferencji</item>
		<item>Aktywność sieciowa wstrzymana</item>
	</string-array>
	<!-- Modes for client (values) - both run and network -->
	<string-array name="clientModeValues">
		<item>1</item>
		<item>2</item>
		<item>3</item>
	</string-array>

	<!-- States of task (all tasks) -->
	<string-array name="resultStates">
		<item>Nowy</item>                 <!-- 0 RESULT_NEW               -->
		<item>Ściąganie</item>         <!-- 1 RESULT_FILES_DOWNLOADING -->
		<item>Ściągniete</item>          <!-- 2 RESULT_FILES_DOWNLOADED  -->
		<item>Błąd obliczeń</item>   <!-- 3 RESULT_COMPUTE_ERROR     -->
		<item>Wysyłanie</item>           <!-- 4 RESULT_FILES_UPLOADING   -->
		<item>Gotowy do raport.</item>     <!-- 5 RESULT_FILES_UPLOADED    -->
		<item>Przerwane</item>             <!-- 6 RESULT_ABORTED           -->
	</string-array>
	<!-- States of task (active tasks only) -->
	<string-array name="activeTaskStates">
		<item>Gotowe do uruch.</item>  <!-- 0 PROCESS_UNINITIALIZED (process doesn't exist yet) -->
		<item>Uruchomione</item>         <!-- 1 PROCESS_EXECUTING (process is running, as far as we know) -->
		<item>Wyjście</item>          <!-- 2 *PROCESS_EXITED -->
		<item>Wyjście</item>          <!-- 3 *PROCESS_WAS_SIGNALED     -->
		<item>Wyjście</item>          <!-- 4 *PROCESS_EXIT_UNKNOWN   -->
		<item>Przerwane</item>         <!-- 5 PROCESS_ABORT_PENDING (process exceeded limits; send "abort" message, waiting to exit)   -->
		<item>Przerwane</item>         <!-- 6 *PROCESS_ABORTED          -->
		<item>Błąd przy starcie</item> <!-- 7 *PROCESS_COULDNT_START   -->
		<item>Wychodzenie</item>         <!-- 8 PROCESS_QUIT_PENDING (we've sent it a "quit" message, waiting to exit) -->
		<item>Czekanie na uruch.</item>  <!-- 9 PROCESS_SUSPENDED (we've sent it a "suspend" message)        -->
	</string-array>

	<string-array name="pollOps">
	    <item>Dołączanie do BAM</item>
