	</string-array>

	<!-- States of task (all tasks) -->
	<string-array name="resultStates">
		<item>Neu</item>                 <!-- 0 RESULT_NEW               -->
		<item>Wird heruntergeladen</item>         <!-- 1 RESULT_FILES_DOWNLOADING -->
		<item>Heruntergeladen</item>          <!-- 2 RESULT_FILES_DOWNLOADED  -->
		<item>Berechnungsfehler</item>   <!-- 3 RESULT_COMPUTE_ERROR     -->
		<item>Wird hochgeladen</item>           <!-- 4 RESULT_FILES_UPLOADING   -->
		<item>Meldebereit</item>     <!-- 5 RESULT_FILES_UPLOADED    -->
		<item>Abgebrochen</item>             <!-- 6 RESULT_ABORTED           -->
	</string-array>
	<!-- States of task (active tasks only) -->
	<string-array name="activeTaskStates">
		<item>Zum ausführen bereit</item>  <!-- 0 PROCESS_UNINITIALIZED (process doesn't exist yet) -->
		<item>Aktiv</item>         <!-- 1 PROCESS_EXECUTING (process is running, as far as we know) -->
		<item>Beendet</item>          <!-- 2 *PROCESS_EXITED -->
		<item>Beendet</item>          <!-- 3 *PROCESS_WAS_SIGNALED     -->
		<item>Beendet</item>          <!-- 4 *PROCESS_EXIT_UNKNOWN   -->
		<item>Abgebrochen</item>         <!-- 5 PROCESS_ABORT_PENDING (process exceeded limits; send "abort" message, waiting to exit)   -->
		<item>Abgebrochen</item>         <!-- 6 *PROCESS_ABORTED          -->
		<item>Fehler beim starten</item> <!-- 7 *PROCESS_COULDNT_START   -->
		<item>Wird beendet</item>         <!-- 8 PROCESS_QUIT_PENDING (we've sent it a "quit" message, waiting to exit) -->
		<item>Zum ausführen bereit</item>  <!-- 9 PROCESS_SUSPENDED (we've sent it a "suspend" message)        -->
	</string-array>
	
